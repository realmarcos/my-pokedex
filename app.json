{
  "expo": {
    "name": "my-pokedex",
    "slug": "my-pokedex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.realmarcos.mypokedex"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "64201df1-bc4a-4ba3-958a-5453d7bb2445"
      }
    }
  }
}
